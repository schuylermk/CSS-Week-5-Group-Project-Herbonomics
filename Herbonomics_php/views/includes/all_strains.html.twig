<table border = "1" cellpadding = "15">
           <tr>
               <th>Strain</th>
               <th>Phenotype</th>
               <th>THC %</th>
               <th>CBD %</th>
               <th>Price ($/lb)</th>
               <th>CGC</th>
               <th>Grower</th>
           </tr>
           {% if strains is not empty %}
               {% for strain in strains %}
               <tr>
                   <td>
                       {{ strain.getStrainName }}
                   </td>
                   <td>
                       {{ strain.getPheno }}
                   </td>
                   <td>
                       {{ strain.getThc }}
                   </td>
                   <td>
                       {{ strain.getCbd }}
                   </td>
                   <td>
                       {{ strain.getPrice }}
                   </td>
                   <td>
                       {% if strain.getCgc == 1 %}
                           <img src="/img/cleangreen.png"  style="height: 3em;" alt="" />

                       {% else %}

                       {% endif %}
                   </td>
                   <td>
                       {{ strain.findGrowerName(strain.getGrowersId) }}
                   </td>
               </tr>
               {% endfor %}
           {% else %}
           <tr>
               <td colspan = "8">
                    No Strains Available
               </td>
           </tr>
           {% endif %}
       </table>
